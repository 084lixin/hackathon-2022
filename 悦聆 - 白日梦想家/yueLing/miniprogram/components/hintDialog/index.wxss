
.dialog-mask{
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.3);
  position: absolute;
  left: 0;
  top: 0;
}
.dialogBox{
  width: 90vw;
  min-height: 350rpx;
  max-height: 400rpx;
  background: rgb(255, 255, 255);
  border-radius: 25rpx;
  box-sizing: border-box;
  position: absolute;
  z-index: 1;
  margin: auto;
  left: -25%;
  right: -25%;
  top: -30%;
  bottom: -25%;

}
.dialogBox text{
display: block;
text-align: center;
}

.dialog-title{
  font-size: 40rpx;
  font-weight: 600;
  margin-top: 50rpx;
  color: rgb(0, 0, 0);
  letter-spacing: 1px;

}
.dialog-content{
  font-size: 34rpx;
  font-weight: 540;
  margin-top: 40rpx;
  color: rgb(134, 123, 123);
  letter-spacing: 1px;

}
.dialog-bottom{
  width: 100%;
  height: 100rpx;
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items:center;
  padding: 20rpx 70rpx;
  font-size: 40rpx;
  box-sizing: border-box;
  margin-bottom: 60rpx;
}
.dialog-bottom-btn{
  padding: 28rpx 80rpx;
  border-radius: 999px;
  box-sizing: 100px 60px;
  display: inherit;

}
.cancel{
  font-size: 18px;
  font-weight: 500;
  background: rgba(245,245,245);
  color: rgb(160, 158, 158);
  letter-spacing: 1px;

}
.confirm{
  font-size: 18px;
  font-weight: 500;
  background:rgba(0, 199, 189);
  color:rgb(255, 255, 255);
  letter-spacing: 1px;

}